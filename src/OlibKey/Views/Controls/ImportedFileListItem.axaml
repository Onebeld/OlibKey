<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OlibKey.Views.Controls.ImportedFileListItem">
	<StackPanel Spacing="5">
		<StackPanel Orientation="Horizontal" Spacing="5">
			<TextBlock Text="{DynamicResource NameColon}"/>
			<TextBlock Text="{Binding Name}"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal" Spacing="5">
			<Button Content="{DynamicResource Export}" Click="ExportFile" Padding="5 2"/>
			<Button x:Name="bDelete" Content="{DynamicResource Delete}" Click="DeleteFile" Padding="5 2"/>
		</StackPanel>
	
		<Separator x:Name="sLine"/>
	</StackPanel>
</UserControl>
